package com.za.websocket;

import java.io.IOException;
import java.net.URISyntaxException;

public class testClientJava {
	public static void main(String[] args) throws URISyntaxException, IOException{
		ClientJava clientJava = new ClientJava();
		
		clientJava.accrocherCapteur("capteur1");
		clientJava.accrocherCapteur("capteur2");
		clientJava.accrocherCapteur("capteur3");
		clientJava.accrocherCapteur("capteur4");
		clientJava.accrocherCapteur("capteur5");
		clientJava.accrocherCapteur("capteur6");
		
		clientJava.envoieMessage("capteur_IR", "12");
		clientJava.envoieMessage("capteur_UV", "2");
		clientJava.envoieMessage("capteur_distance", "17");
		clientJava.envoieMessage("capteur_giroscope", "6");
		clientJava.envoieMessage("capteur_gps", "20");
		clientJava.envoieMessage("capteur_wifi", "9");
		clientJava.fermerClient();
	}
}
